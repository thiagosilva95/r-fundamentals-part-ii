?do.call
data <- list()
N <- 100
data[[n]] = data.frame(index = n, char = sample(lettersm, 1), z = rnorm(1))
for (n in 1:N) {
data[[n]] = data.frame(index = n, char = sample(lettersm, 1), z = rnorm(1))
}
for (n in 1:N) {
data[[n]] = data.frame(index = n, char = sample(letters, 1), z = rnorm(1))
}
head(data)
do.call(rbind, data)
class(do.call(rbind, data))
